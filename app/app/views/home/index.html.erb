
<div class="container">

<h1>NOppression News</h1>
<h4>Not Today. Not Even On My Worst Day.</h4>



	<div class="row">

		<div class="col-12">

<p id="notice"><%= notice %></p>



<table class="table">
  <thead>
    <tr>
      <th scope="col">User</th>
      <th scope="col">Url</th>
      <th scope="col">Upvotes</th>
      <th scope="col">Headline</th>
      <th scope="col">Body</th>
  

  <tbody>
    <% @posts.each do |post| %>
      <tr>
      	<th scope="row">1</th>
        <td><%= post.user_id %></td>
        <td><%= post.url %></td>
        <td><%= post.upvotes %></td>
        <td><%= post.headline %></td>
        <td><%= post.body %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



		</div>

	</div>
